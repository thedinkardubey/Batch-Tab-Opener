{
    "manifest_version": 3,
    "name": "Batch Tab Opener",
    "version": "1.0",
    "description": "A Chrome extension to bulk open websites.",
    "permissions": ["tabs"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon16.png",
            "48": "icon48.png",
            "128": "icon128.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}
